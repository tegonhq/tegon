{
  "name": "server",
  "version": "0.2.0",
  "description": "Tegon server",
  "author": "Tegon",
  "private": true,
  "license": "MIT",
  "keywords": [
    "nestjs",
    "prisma",
    "prisma client",
    "typescript",
    "passport"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tegonhq/server.git"
  },
  "bugs": {
    "url": "https://github.com/tegonhq/server.git/issues"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start-prod": "pnpm run migrate:deploy && pnpm run db:generate && node dist/src/main",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:dev": "npx prisma migrate dev --preview-feature",
    "migrate:dev:create": "npx prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "npx prisma migrate reset --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "db:generate": "npx prisma generate",
    "create-resources": "node scripts/createUserWorkspaceTeam",
    "deploy-trigger-tasks": "npx trigger.dev@beta deploy --self-hosted"
  },
  "dependencies": {
    "@devoxa/prisma-relay-cursor-connection": "3.1.1",
    "@google-cloud/storage": "^7.10.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bull": "^10.1.0",
    "@nestjs/common": "10.3.1",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.3.1",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.3.1",
    "@nestjs/platform-socket.io": "^10.3.1",
    "@nestjs/swagger": "7.2.0",
    "@nestjs/websockets": "^10.3.1",
    "@prisma/client": "^5.8.1",
    "@tiptap/core": "^2.3.0",
    "@tiptap/extension-blockquote": "^2.4.0",
    "@tiptap/extension-bullet-list": "^2.4.0",
    "@tiptap/extension-code-block": "^2.4.0",
    "@tiptap/extension-document": "^2.4.0",
    "@tiptap/extension-hard-break": "^2.4.0",
    "@tiptap/extension-heading": "^2.4.0",
    "@tiptap/extension-horizontal-rule": "^2.4.0",
    "@tiptap/extension-image": "^2.4.0",
    "@tiptap/extension-link": "^2.4.0",
    "@tiptap/extension-list-item": "^2.4.0",
    "@tiptap/extension-ordered-list": "^2.4.0",
    "@tiptap/extension-paragraph": "^2.4.0",
    "@tiptap/extension-task-item": "^2.4.0",
    "@tiptap/extension-task-list": "^2.4.0",
    "@tiptap/extension-text": "^2.4.0",
    "@tiptap/extension-underline": "^2.4.0",
    "@tiptap/html": "^2.4.0",
    "@tiptap/pm": "^2.4.0",
    "@trigger.dev/sdk": "3.0.0-beta.50",
    "@types/multer": "^1.4.11",
    "@vegardit/prisma-generator-nestjs-dto": "^1.5.1",
    "@xenova/transformers": "^2.16.1",
    "axios": "^1.6.7",
    "bcrypt": "5.1.1",
    "bull": "^4.12.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cohere-ai": "^7.10.6",
    "cookie": "^0.6.0",
    "cron": "^3.1.7",
    "csv-writer": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "knex": "^3.1.0",
    "nestjs-prisma": "0.22.0",
    "nodemailer": "^6.9.13",
    "ollama": "^0.5.6",
    "openai": "^4.28.4",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pg": "^8.12.0",
    "pg-logical-replication": "^2.0.3",
    "prosemirror-model": "^1.20.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "7.8.1",
    "simple-oauth2": "^5.0.0",
    "socket.io": "^4.7.5",
    "supertokens-node": "^16.7.1",
    "typesense": "^1.7.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@tegonhq/eslint-config": "workspace:*",
    "@tegonhq/typescript-config": "workspace:*",
    "@nestjs/cli": "10.3.0",
    "@nestjs/schematics": "10.1.0",
    "@nestjs/testing": "10.3.1",
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.3.105",
    "@types/bcrypt": "5.0.2",
    "@types/chance": "1.1.6",
    "@types/cookie": "^0.6.0",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.11.0",
    "@types/simple-oauth2": "^5.0.7",
    "@types/supertest": "6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "chance": "1.1.11",
    "eslint": "^8.56.0",
    "jest": "29.7.0",
    "prettier": "^3.2.4",
    "prisma": "5.8.1",
    "prisma-dbml-generator": "0.10.0",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
