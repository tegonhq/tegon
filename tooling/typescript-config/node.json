{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 18",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": [ "ES2022" ],
    "module": "commonjs",
    "target": "ES2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}