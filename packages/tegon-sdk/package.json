{
  "name": "@tegonhq/sdk",
  "version": "0.1.0-beta.2",
  "description": "Tegon Node.JS SDK",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:tsup",
    "build:tsup": "tsup --dts-resolve",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@trigger.dev/sdk": "3.0.0-beta.50",
    "axios": "^1.6.7",
    "chalk": "^5.2.0",
    "configstore": "^7.0.0",
    "cronstrue": "^2.21.0",
    "debug": "^4.3.4",
    "evt": "^2.4.13",
    "slug": "^6.0.0",
    "terminal-link": "^3.0.0",
    "uuid": "^9.0.0",
    "zod": "3.22.3"
  },
  "devDependencies": {
    "@tegonhq/services": "workspace:*",
    "@tegonhq/types": "workspace:*",
    "@tegonhq/typescript-config": "workspace:*",
    "@types/configstore": "^6.0.2",
    "@types/debug": "^4.1.7",
    "@types/node": "18",
    "@types/slug": "^5.0.3",
    "@types/uuid": "^9.0.0",
    "encoding": "^0.1.13",
    "rimraf": "^3.0.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}